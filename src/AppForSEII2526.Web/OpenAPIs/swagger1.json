        "required": [
          "customerCountry",
          "reviewItems",
          "reviewTitle"
        ],
        "type": "object",
        "properties": {
          "reviewTitle": {
            "minLength": 1,
            "type": "string"
          },
          "customerCountry": {
            "minLength": 1,
            "type": "string"
          },
          "username": {
            "type": "string",
            "nullable": true
          },
          "reviewItems": {
            "minItems": 1,
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ReviewItemDTO"
            }
          }
        },
        "additionalProperties": false
      },
      "DeviceForRentalDTO": {
        "required": [
          "priceForRent"
        ],
        "type": "object",
        "properties": {
          "nombre": {
            "type": "string",
            "nullable": true
          },
          "modelName": {
            "type": "string",
            "nullable": true
          },
          "brand": {
            "type": "string",
            "nullable": true
          },
          "year": {
            "type": "integer",
            "format": "int32"
          },
          "color": {
            "type": "string",
            "nullable": true
          },
          "priceForRent": {
            "minimum": 1,
            "type": "number",
            "format": "currency"
          }
        },
        "additionalProperties": false
      },
      "DevicesRese単aDTO": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "brand": {
            "type": "string",
            "nullable": true
          },
          "color": {
            "type": "string",
            "nullable": true
          },
          "year": {
            "type": "integer",
            "format": "int32"
          },
          "model": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "PaymentMethodType": {
        "enum": [
          "CreditCard",
          "Paypal",
          "Cash"
        ],
        "type": "string"
      },
      "ProblemDetails": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "detail": {
            "type": "string",
            "nullable": true
          },
          "instance": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": { }
      },
      "RentDeviceDTO": {
        "type": "object",
        "properties": {
          "deviceModel": {
            "type": "string",
            "nullable": true
          },
          "pricePerDay": {
            "type": "number",
            "format": "double"
          },
          "quantity": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "RentalDetailsDTO": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "surname": {
            "type": "string",
            "nullable": true
          },
          "deliveryAddress": {
            "type": "string",
            "nullable": true
          },
          "rentalDate": {
            "type": "string",
            "format": "date-time"
          },
          "totalPrice": {
            "type": "number",
            "format": "double"
          },
          "rentalPeriodDays": {
            "type": "integer",
            "format": "int32"
          },
          "rentalItems": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/RentDeviceDTO"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "RentalForCreateDTO": {
        "required": [
          "deliveryAddress",
          "name",
          "paymentMethod",
          "quantity",
          "surname"
        ],
        "type": "object",
        "properties": {
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "surname": {
            "minLength": 1,
            "type": "string"
          },
          "deliveryAddress": {
            "maxLength": 50,
            "minLength": 2,
            "type": "string",
            "format": "multiline"
          },
          "paymentMethod": {
            "$ref": "#/components/schemas/PaymentMethodType"
          },
          "quantity": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "ReviewDTO": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "customerCountry": {
            "type": "string",
            "nullable": true
          },
          "reviewTitle": {
            "type": "string",
            "nullable": true
          },
          "dateOfReview": {
            "type": "string",
            "format": "date-time"
          },
          "reviewItems": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ReviewItemDTO"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ReviewItemDTO": {
        "type": "object",
        "properties": {
          "deviceId": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "rating": {
            "maximum": 5,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "comment": {
            "type": "string",
            "nullable": true
          },
          "deviceName": {
            "type": "string",
            "nullable": true
          },
          "deviceModel": {
            "type": "string",
            "nullable": true
          },
          "deviceYear": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "ValidationProblemDetails": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "detail": {
            "type": "string",
            "nullable": true
          },
          "instance": {
            "type": "string",
            "nullable": true
          },
          "errors": {
            "type": "object",
            "additionalProperties": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "nullable": true
          }
        },
        "additionalProperties": { }
      }
